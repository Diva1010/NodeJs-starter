<!DOCTYPE html>
<html lang="en">

<body class="container">

    <header>
        <% include ./header %>
    </header>

    <main>
        <div>
            <h2>User Details</h2>
            <p> Note:
                <%= note %>
            </p>
            <table class="table table-striped table-bordered">
                <thead>
                    <tr>
                        <th>Name</th>
                        <th>Age</th>
                        <th>Address</th>
                    </tr>
                </thead>
                <tbody <% Allusers.forEach(function(user) { %>
                    <tr>
                        <td>
                            <%= user.name %>
                        </td>
                        <td>
                            <%= user.age %>
                        </td>
                        <td>
                            <%= user.address %>
                        </td>
                    </tr>
                    <% }); %>
                </tbody>
            </table>


        </div>
    </main>

</body>

</html>